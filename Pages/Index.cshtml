@page
@model CzechsInNHL.Pages.IndexModel

@{
    ViewData["Title"] = "Home page";
}

<div class="band">
   @foreach (var item in Model.Players)
    {
        <div class="item-1">
            <a href="https://design.tutsplus.com/articles/international-artist-feature-malaysia--cms-26852" class="card">
                <div class="thumb" style="background-image: url(../images/PlayerProfilePhotos/pasta.jpg);"></div>
                <article>
                    <h1>@Html.DisplayFor(modelItem => item.fullName)</h1>
                     <span>@Html.DisplayFor <span>
                </article>
            </a>
        </div>


    }
</div>